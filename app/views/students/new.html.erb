<!-- Create your form here -->
<h1>Student Form</h1>
<br>
<%= form_for @student do |f| %>
  <%= f.label :first_name %><br>
  <%= f.text_field :first_name, :id => "first_name" %><br>
  <br>
  <%= f.label :last_name %><br>
  <%= f.text_field :last_name, :id => "last_name" %><br>
  <br>
  <%= f.submit "Submit Student"%>
<% end %>
